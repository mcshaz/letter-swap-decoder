{
  "name": "vite-vue-starter",
  "version": "0.1.3",
  "author": {
    "name": "Brent McSharry"
  },
  "license": "MIT",
  "description": "Assist decoding simple letter for letter substitution ciphers in English based on individual letter frequency",
  "repository": {
    "url": "github.com/mcshaz/letter-swap-decoder"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@stdlib/stats-base-dists-binomial-pmf": "^0.0.7",
    "pinia": "^2.0.23",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.1",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.27.0",
    "eslint-plugin-vue": "^9.7.0",
    "jsdom": "^20.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vitest": "^0.25.1",
    "vue-tsc": "^1.0.9"
  }
}
